<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="255.0" prefWidth="530.0" stylesheets="@../style/StyleSheet.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep10.db.controller.UserWindowController">
   <children>
      <VBox alignment="CENTER" layoutX="28.0" layoutY="45.0" minWidth="430.0" prefHeight="178.0" prefWidth="475.0" spacing="15.0" AnchorPane.leftAnchor="28.0" AnchorPane.rightAnchor="27.0">
         <children>
            <Label text="User Window">
               <font>
                  <Font name="System Bold Italic" size="25.0" />
               </font>
            </Label>
            <HBox fx:id="hboxUserTime">
               <children>
                  <Label fx:id="lblTime" prefHeight="16.0" prefWidth="314.0" text="TIME" HBox.hgrow="ALWAYS" />
                  <Label fx:id="lblUserName" alignment="CENTER_RIGHT" prefHeight="16.0" prefWidth="202.0" text="USER NAME" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <Button fx:id="btnManageCustomers" mnemonicParsing="false" onAction="#btnManageCustomersOnActions" prefHeight="24.0" prefWidth="477.0" text="MANAGE CUSTOMERS" />
            <Button fx:id="btnManageItems" mnemonicParsing="false" onAction="#btnManageItemsOnActions" prefHeight="24.0" prefWidth="480.0" text="MANAGE ITEMS" />
            <Button fx:id="btnLogOut" mnemonicParsing="false" onAction="#btnLogOutOnAction" text="LOG OUT" />
         </children>
      </VBox>
   </children>
</AnchorPane>
