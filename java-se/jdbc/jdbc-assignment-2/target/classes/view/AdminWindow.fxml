<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="346.0" prefWidth="539.0" stylesheets="@../style/StyleSheet.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep10.db.controller.AdminWindowController">
   <children>
      <VBox alignment="CENTER" layoutX="35.0" layoutY="45.0" prefHeight="217.0" prefWidth="470.0" spacing="15.0">
         <children>
            <Label text="Admin Window">
               <font>
                  <Font name="System Bold Italic" size="25.0" />
               </font>
            </Label>
            <HBox fx:id="hboxUserTime" alignment="CENTER" prefHeight="16.0" prefWidth="403.0" spacing="50.0">
               <children>
                  <Label fx:id="lblTime" prefHeight="16.0" prefWidth="264.0" text="TIME" />
                  <Label fx:id="lblUserName" alignment="TOP_RIGHT" prefHeight="16.0" prefWidth="210.0" text="USER NAME" />
               </children>
            </HBox>
            <Button fx:id="btnManageCustomer" mnemonicParsing="false" onAction="#btnManageCustomerOnAction" prefHeight="24.0" prefWidth="479.0" text="MANAGE CUSTOMER" />
            <Button fx:id="btnManageItems" mnemonicParsing="false" onAction="#btnManageItemsOnActions" prefHeight="24.0" prefWidth="470.0" text="MANAGE ITEMS" />
            <Button fx:id="btnMoveUsers" mnemonicParsing="false" onAction="#btnMoveUsersOnAction" prefHeight="24.0" prefWidth="543.0" text="MANAGE USERS" />
            <Button fx:id="btnLogOut" mnemonicParsing="false" onAction="#btnLogOutOnAction" text="LOG OUT" />
         </children>
      </VBox>
   </children>
</AnchorPane>
